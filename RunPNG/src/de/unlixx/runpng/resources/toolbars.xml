<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE toolbars PUBLIC "-//unlixx.de//RunPNG//EN" "menusandbars.dtd">
<toolbars>
	<toolbar id="toolbar.main">
		<imagebutton id="menu.file.new" icon="icons/32x32/file.new.png" tooltip="tooltip.file.new" />
		<separator />
		<imagebutton id="menu.file.open" icon="icons/32x32/file.open.png" tooltip="tooltip.file.open" />
		<imagebutton id="menu.files.open" icon="icons/32x32/files.open.png" tooltip="tooltip.files.open" />
